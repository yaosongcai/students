<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="DropDownButton">
        <attr name="gravity_mode">
            <enum name="center" value="0"/>
            <enum name="left" value="1"/>
            <enum name="right" value="2"/>
        </attr>
        <attr name="drop_text" format="string"/>
        <attr name="drop_textColor" format="color"/>
        <attr name="drop_image" format="reference"></attr>
    </declare-styleable>
</resources>